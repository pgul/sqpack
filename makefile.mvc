# Microsoft Visual C++ Makefile

CC=     cl
CFLAGS= -nologo -Zp1 -I.. -Ih
CDEFS=  -D__NT__ -DWIN32 -D_CONSOLE -DINTEL
COPT=   -O2
TLIB=   lib -nologo

SMAPIL=	..\smapi\smapimvc.lib
FCONFL=	..\fidoconf\fconfmvc.lib

all: sqpack.exe

sqpack.exe:
        $(CC) $(CFLAGS) $(CDEFS) $(COPT) -Fe$@ sqpack.c $(FCONFL) $(SMAPIL)

clean:
	-del *.obj

distclean: clean
	-del *.exe
